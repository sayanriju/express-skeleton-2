{
  "name": "express-skeleton-2",
  "version": "2.0.0",
  "description": "A starter template for Express JS apps, with sane defaults",
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sayanriju/express-skeleton-2"
  },
  "main": "bin/www",
  "author": "Sayan \"Riju\" Chakrabarti <s26c.sayan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "lint": "npm run lint:quiet",
    "lint:quiet": "eslint --quiet './**/*.js'",
    "lint:all": "eslint './**/*.js'",
    "lint:fix": "eslint './**/*.js' --quiet --fix",
    "test:nocoverage": "ava",
    "test:withcoverage": "nyc ava",
    "test": "npm run test:withcoverage",
    "gendoc": "apidoc -i routes/ -o public/docs"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.2",
    "email-templates": "^6.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.27.0",
    "mongodb-memory-server": "^3.1.3",
    "mongoose": "^5.10.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-mocks-http": "^1.9.0",
    "randomstring": "^1.1.5",
    "serve-favicon": "^2.5.0",
    "tinyurl": "^1.1.7"
  },
  "eslintIgnore": [
    "public/",
    "bin/"
  ],
  "devDependencies": {
    "apidoc": "^0.17.7",
    "ava": "^3.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "nodemon": "^2.0.4",
    "nyc": "^14.1.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:quiet"
    }
  }
}
